Algoritimo
VAR
    INTEIRO: num, antecessor, sucessor;
INICIO
    ESCREVA("Digite um valor para ver seu antecessor e sucessor: );
    LEIA(num);

    antecessor <- num - 1;

    sucessor <- num + 1;

    ESCREVA("O antecessor de:", num , "é:", antecessor, "o sucessor será:", sucessor. );
FIM